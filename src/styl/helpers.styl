button-reset()
    box-sizing content-box;
    background none;
    border 0;
    color inherit;
    font inherit;
    line-height normal;
    overflow visible;
    outline none;
    padding 0;
    -webkit-appearance button; /* for input */
    -webkit-user-select none; /* for button */
    -moz-user-select none;
    -ms-user-select none;
